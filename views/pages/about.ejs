<%- include('templates/header') %>
    <main>
    <!--? Hero Start -->
    <div class="slider-area2">
        <div class="slider-height2 d-flex align-items-center">
            <div class="container">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="hero-cap hero-cap2 pt-20 text-center">
                            <h2>About US</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Hero End -->
   
    <!--? About Law Start-->
    <section class="about-low-area section-padding2">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-10">
                    <div class="about-caption mb-50">
                        <!-- Section Tittle -->
                        <div class="section-tittle mb-35">
                            <span>Karpearl</span>
                            <h2> Medical Foundation</h2>
                        </div>
                        <p>Karpearl Medical Foundation was established on the 15th of March 2014, with the goal of bridging the healthcare gap for vulnerable populations.Our team of dedicated healthcare professionals and volunteers has been working tirelessly to provide quality medical care to those in need. </p>
                        <p></p>
                    </div>
                    <!--<a href="about.html" class="btn">About US</a>-->
                </div>
                <div class="col-lg-6 col-md-12">
                    <!-- about-img -->
                     <div class="about-caption mb-50">
                        <!-- Section Tittle -->
                        <div class="section-tittle mb-35">
                            <span>What do we do</span>
                            <h2>Our Work</h2>
                        </div>
                        <p>We provide a range of medical services, including free HIV treatment and counseling,free dental and eye treatment, free laboratory investigations, free hypertensive treatment, malaria treatment, and emergency care for people living with HIV and AIDS.</p>
                        <p> </p>
                    </div>
                </div>
                <div class="col-lg-6 col-md-12">
                    <!-- about-img -->
                     <div class="about-caption mb-50">
                        <!-- Section Tittle -->
                        <div class="section-tittle mb-35">
                            <!--<span>What do we do</span>-->
                            <h2>Mission Statement</h2>
                        </div>
                        <p>Karpearl Medical Foundation is dedicated to providing free medical treatment, care, and support to the less privileged, with a focus on those living with HIV and AIDS.</p>
                        <p> </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- About Law End-->
   
  

    <!--? Count Down Start -->
    <div class="count-down-area pt-25 section-bg" data-background="assets/img/gallery/section_bg02.png">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-12 col-md-12">
                    <div class="count-down-wrapper" >
                        <div class="row justify-content-between">
                              <% getnumInfo.forEach(function(getnum) { %>
                                     <% const getv = parseInt(getnum.volunteers) %>
                                     <% const getT = parseInt(getnum.treated) %>
                                     <% const  getD = parseInt(getnum.donations) %>

                                     <div class="col-lg-3 col-md-6 col-sm-6">
                                    <!-- Counter Up -->
                                    <div class="single-counter text-center">
                                        <span class="counter color-green"><%= getv %></span>
                                        <span class="plus">+</span>
                                        <p class="color-green">Volunteers</p>
                                    </div>
                                </div>

                                <div class="col-lg-3 col-md-6 col-sm-6">
                                    <!-- Counter Up -->
                                    <div class="single-counter text-center">
                                        <span class="counter color-green"><%= getT %></span>
                                        <span class="plus">+</span>
                                        <p class="color-green">Africans Treated</p>
                                    </div>
                                </div>

                                <div class="col-lg-3 col-md-6 col-sm-6">
                                    <!-- Counter Up -->
                                    <div class="single-counter text-center">
                                        <span class="counter color-green"><%= getnum.donations %></span>
                                        <span class="plus">+</span>
                                        <p class="color-green">Donation</p>
                                    </div>
                                </div>
                                 <% }) %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Count Down End -->
    </main>
  <%- include('templates/footer') %>